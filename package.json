{
  "name": "@pixore/window",
  "version": "0.1.0",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist && rm -rf .cache & rm tsconfig.tsbuildinfo",
    "prebuild": "yarn clean",
    "build": "tsc",
    "start": "parcel examples/index.html",
    "test:types": "tsc --noEmit",
    "test:duplication": "jscpd .",
    "test:cy": "start-server-and-test start http://localhost:1234 cy:run",
    "test": "run-s test:types test:duplication test:cy",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "prepack": "yarn build",
    "release": "standard-version && git push --follow-tags origin HEAD"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "license": "MIT",
  "dependencies": {
    "@reach/portal": "^0.15.0"
  },
  "devDependencies": {
    "@pixore/prettier-config": "0.0.2",
    "@types/once": "~1.4.0",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "cypress": "~3.4.1",
    "jscpd": "^3.3.26",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "standard-version": "^7.0.1",
    "start-server-and-test": "^1.10.4",
    "typescript": "4.3.2"
  }
}
